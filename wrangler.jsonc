{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "availability-dash",
	"main": "src/index.js",
	"compatibility_date": "2025-12-30",
	"observability": {
		"enabled": true
	},
	"assets": {
		"directory": "./dist/",
		"binding": "ASSETS",
		"not_found_handling": "single-page-application",
		"run_worker_first": [
			"/status"
		]
	},
	"vars": {
		"BACKEND_HOSTNAME": "URL TO HOSTED RegularRabbit/availability",
	},
	"placement": {
		"mode": "smart"
	}
}